{
  "name": "bavix/laravel-wallet-vacuum",
  "description": "Addition to the package laravel-wallet.",
  "keywords": [
    "vacuum",
    "laravel",
    "laravel6",
    "laravel7",
    "credits",
    "bavix",
    "currency",
    "laravel6-package",
    "laravel7-package",
    "laravel-wallet",
    "virtual",
    "wallet",
    "payments"
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/bavix/laravel-wallet",
  "license": "MIT",
  "authors": [
    {
      "name": "Babichev Maxim",
      "email": "info@babichev.net"
    }
  ],
  "require": {
    "php": "^7.3",
    "bavix/laravel-wallet": "6.x-dev"
  },
  "require-dev": {
    "infection/infection": "0.17.*",
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpstan/phpstan": "^0.12",
    "phpunit/phpunit": "^8.0|^9.0",
    "laravel/cashier": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Bavix\\WalletVacuum\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bavix\\WalletVacuum\\Test\\": "tests/",
      "Bavix\\Wallet\\Test\\": "vendor/bavix/laravel-wallet/tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bavix\\WalletVacuum\\VacuumServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
